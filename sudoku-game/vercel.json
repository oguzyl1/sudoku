{
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/sudoku-game/backend/api/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/sudoku-game/client/index.html"
      }
    ],
    "builds": [
      {
        "src": "/sudoku-game/backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "/sudoku-game/client/dist"
        }
      }
    ]
  }
  